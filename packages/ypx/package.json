{
  "name": "ynpx",
  "version": "1.0.31",
  "description": "`npx` equivalent in yarn",
  "keywords": [
    "yarn",
    "run",
    "local",
    "bin",
    "scripts",
    "script",
    "package",
    "project",
    "npx",
    "dlx",
    "npm",
    "npm exec",
    "shell",
    "npm bin",
    "cli",
    "ypx",
    "ynpx",
    "yypx",
    "yarn-tool",
    "npm run",
    "yarn run",
    "alias",
    "global"
  ],
  "homepage": "https://github.com/bluelovers/ws-ypx/tree/master/packages/ypx#readme",
  "bugs": {
    "url": "https://github.com/bluelovers/ws-ypx/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bluelovers/ws-ypx.git"
  },
  "license": "ISC",
  "author": "",
  "main": "index.js",
  "bin": {
    "ynpx": "bin/ypx.js",
    "ypx": "bin/ypx.js",
    "yypx": "bin/ypx.js"
  },
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "coverage": "npx nyc yarn run test",
    "lint": "npx eslint **/*.ts",
    "pretest": "yarn run build",
    "test": "jest --coverage",
    "test:update-snapshots": "yarn run test -- -u",
    "build": "babel index.ts -o index.js --ignore \"*.d.ts\" && babel ./bin -x .ts -d ./bin --ignore \"*.d.ts\" && babel ./lib -x .ts -d ./lib --ignore \"*.d.ts\"",
    "build:add": "git add ./bin & git add ./lib & git add ./index.js & git add ./cli.md",
    "build:all": "yarn run build && yarn run build:add && yarn run build:commit",
    "build:commit": "git commit -a -m builded",
    "npm:publish": "npm publish",
    "prepublish:lockfile": "ynpx sync-lockfile -- . && del package-lock.json && del npm-shrinkwrap.json && npm shrinkwrap && yarn-tool lockfile --npm --overwrite",
    "prepublishOnly": "yarn run build && yarn run build:add && yarn run test",
    "prepublishOnly:all": "yarn run ncu && npx yarn-tool install && yarn run prepublish:lockfile && yarn run prepublishOnly && yarn run sort-package-json & yarn run cli:help & yarn run build:add & yarn run build:commit",
    "prepublishOnly_": "yarn run ncu && yarn run sort-package-json && yarn run test",
    "postpublish_": "git commit -m \"chore(release): publish\" .",
    "cli:help": "ts-node bin/ypx -h",
    "ncu": "npx yarn-tool ncu -u",
    "sort-package-json": "npx yarn-tool sort",
    "tsc:default": "tsc -p tsconfig.json",
    "tsc:esm": "tsc -p tsconfig.esm.json"
  },
  "dependencies": {
    "@yarn-tool/get-pkg-bin": "^1.0.13",
    "@yarn-tool/update-notifier": "^1.0.12",
    "bin-exists": "^0.2.1",
    "bluebird": "^3.7.2",
    "camel-case": "^4.1.1",
    "crlf-normalize": "^1.0.3",
    "cross-spawn-extra": "^2.1.0",
    "debug-color2": "^1.2.3",
    "fs-extra": "^9.0.1",
    "lodash": "^4.17.15",
    "path-env": "^0.0.12",
    "tmp": "^0.2.1",
    "ts-type": "^1.2.23",
    "yargs": "^15.3.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-proposal-decorators": "^7.10.1",
    "@babel/plugin-proposal-optional-chaining": "^7.10.1",
    "@babel/plugin-proposal-unicode-property-regex": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-typescript": "^7.10.1",
    "@types/babel__core": "^7.1.8",
    "@types/bluebird": "^3.5.32",
    "@types/fs-extra": "^9.0.1",
    "@types/jest": "^26.0.0",
    "@types/lodash": "^4.14.155",
    "@types/react": "^16.9.36",
    "@types/tmp": "^0.2.0",
    "@types/yargs": "^15.0.5",
    "babel-plugin-const-enum": "^1.0.1",
    "jest": "^26.0.1",
    "ts-jest": "^26.1.0"
  },
  "peerDependencies": {
    "yarn": "^1.22.4"
  },
  "gitHead": "89fa13ebd308fadf02d90f7642daca4b8182c6ec"
}
